#
# begin .screenrc-gitdevel-all specific shtuff
# (NOTE: the c-M character says to execute statement (presses ENTER))
screen -t "man"
stuff "cd $HOME/dev/git/"

screen -t "fed"
stuff "cd $HOME/dev/git/gadget-hr-fed"

screen -t "nfo"
stuff "cd $HOME/dev/git/gadget-hr-info"

screen -t "Op"
stuff "cd $HOME/dev/git/gadget-operations"

screen -t "pda"
stuff "cd $HOME/dev/git/gadget-pda"

screen -t "SpryRC"
stuff "cd $HOME/dev/git/SpryRestSvc"

screen -t "~"
stuff "cd $HOME/dev/git/"

screen -t "~"
stuff "cd $HOME/dev/git/"

