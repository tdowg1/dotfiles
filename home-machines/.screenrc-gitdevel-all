# custom screenrc 'execute stuff' help src:
# 
#
# source default/base screenrc config
source $HOME/.screenrc


#
# begin .screenrc-gitdevel-all specific shtuff
# (NOTE: the c-M character says to execute statement (presses ENTER))
screen -t "man"
stuff "cd $HOME/dev/git/sprySuperGitRepo/"

screen -t "Fed"
stuff "cd $HOME/dev/git/sprySuperGitRepo/gadget-hr-fed"

screen -t "Nfo"
stuff "cd $HOME/dev/git/sprySuperGitRepo/gadget-hr-info"

screen -t "Op"
stuff "cd $HOME/dev/git/sprySuperGitRepo/gadget-operations"

screen -t "!Pda!"
#stuff "cd $HOME/dev/git/sprySuperGitRepo/gadget-pda"
stuff "cd $HOME/dev/git/gadget-pda"

screen -t "be"
stuff "cd $HOME/dev/git/sprySuperGitRepo/gadgets-bea/"

screen -t "p2"
stuff "cd $HOME/dev/git/sprySuperGitRepo/p2p"

screen -t "SparqPS"
stuff "cd $HOME/dev/git/sprySuperGitRepo/SparqlProxyService"

screen -t "SpryRC"
stuff "cd $HOME/dev/git/sprySuperGitRepo/SpryRestSvc"

screen -t "~"
stuff "cd $HOME/dev/git/sprySuperGitRepo/"

